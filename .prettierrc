{
  "trailingComma": "all",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "quoteProps": "consistent",
  "jsxSingleQuote": false,
  "printWidth": 100,
  "bracketSpacing": true,
  "bracketSameLine": true,
  "arrowParens": "always",
  "endOfLine": "lf",
  "importOrder": [
    "^react",
    "<THIRD_PARTY_MODULES>",
    "^@components/(.*)$",
    "^components/(.*)$",
    ".(css|scss)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrderTypeScriptVersion": "5.1.6",
  "plugins": [
    "@trivago/prettier-plugin-sort-imports"
  ]
}